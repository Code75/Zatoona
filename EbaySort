javascript:var mylist=[];$('#ListViewInner li.sresult').each(function(){mylist.push($(this)[0])});var list=mylist.filter(function(f){if($(f).find('.hotness-signal').text().indexOf('sold')!=-1){$(f).remove();return f}});list.sort(function(a,b){var compA=parseInt($(a).find('.hotness-signal').text().trim());var compB=parseInt($(b).find('.hotness-signal').text().trim());return(compA==compB)?0:(compA<compB)?-1:1});$.each(list,function(idx,itm){$('#ListViewInner').prepend($(itm))});$('#tech_cp').html('<button onclick=\'location.reload();\'>Reload</button>');window.scrollTo(0,0);
